<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HoneycombButton">
        <attr name="honeycombButtonIcon" format="integer" />
        <attr name="honeycombButtonText" format="string" />
        <attr name="honeycombButtonTextSize" format="dimension" />
        <attr name="honeycombSecondaryText" format="string" />
    </declare-styleable>

    <declare-styleable name="HoneycombSwitch">
        <attr name="honeycombSwitchText" format="string" />
        <attr name="honeycombSwitchTextSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="UIButton">
        <attr name="tintColorList" format="reference|color" />
        <attr name="privateModeTintColorList" format="reference|color" />
        <attr name="activeModeTintColorList" format="reference|color" />
        <attr name="privateModeBackground" format="integer" />
        <attr name="activeModeBackground" format="integer" />
    </declare-styleable>

    <attr name="description" format="string" />

    <declare-styleable name="SwitchSetting">
        <attr name="description" />
    </declare-styleable>

    <declare-styleable name="RadioGroupSetting">
        <attr name="description" />
        <attr name="options" format="reference" />
        <attr name="values" format="reference" />
        <attr name="layout" format="reference" />
    </declare-styleable>

    <declare-styleable name="EditSetting">
        <attr name="description" />
        <attr name="by" />
        <attr name="highlightedTextColor" />
        <attr name="android:width" />
        <attr name="android:maxLength" />
        <attr name="android:inputType" />
    </declare-styleable>

    <declare-styleable name="ButtonSetting">
        <attr name="description" />
        <attr name="buttonText" format="string" />
    </declare-styleable>
</resources>